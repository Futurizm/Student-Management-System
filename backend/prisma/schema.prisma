generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id            Int    @id @default(autoincrement())
  ei_contingent String
  id_contingent String
  iin           String @unique
  surname       String
  name          String
  middlename    String
  birthdate     DateTime
  gender        String
  citizenship   String
  nationality   String
}
